{
  "expo": {
    "name": "alx-online-groceries-app",
    "slug": "alx-online-groceries-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/onboarding-hero.jpg",
    "scheme": "alxonlinegroceriesapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/onboarding-hero.jpg",
        "backgroundImage": "./assets/images/onboarding-hero.jpg",
        "monochromeImage": "./assets/images/onboarding-hero.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.enyasystem.alxonlinegroceriesapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/onboarding-hero.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/onboarding-hero.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
